<!--list.wxml-->
<view class="container netlist">
    <scroll-view class="netlist">
      <radio-group class="radio-group" bindchange="radioChange">
        <label class="radio" wx:for="{{list}}">
          <radio value="{{index}}" checked="{{item}}" />{{item}}
        </label>
      </radio-group>
    </scroll-view>
</view>

<view class="button-wrapper">
  <button type="primary" size="{{warnSize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="paidui"> 在此网点排号
  </button>
</view>
